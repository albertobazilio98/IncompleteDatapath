Entity MUX is
    port(
        D0,D1:IN BIT;
        E0,E1,E2,E3 : in bit_vector(3 downto 0);
        R : out bit_vector(3 downto 0)
    );
END MUX;

Architecture SELECTOR of MUX is
begin
    process(D0,D1,E0,E1,E2,E3)
    begin
        if(D0 = '0' AND D1 = '0') then
            R <= E0;
        elsif(D0 = '0' AND D1 = '1') then
            R <= E1;
        elsif(D0 = '1' AND D1 = '0') then
            R <= E2;
        else
            R <= E3;
        end if;
    end process;
end SELECTOR;